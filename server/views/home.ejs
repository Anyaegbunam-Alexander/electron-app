<h1>Users</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">About</th>
    </tr>
  </thead>
  <tbody>
    <% names.forEach(function(name) { %>
      <tr>
        <th scope="row"><%= name.id %></th>
        <td><a href="/user/<%= name.id %>"><%= name.name %></a></td>
        <td><%= name.about %></td>
      </tr>
    <% }); %>
  </tbody>
</table>
